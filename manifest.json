{
  "name": "Reddit Summarize",
  "version": "1.2",
  "description": "A browser action to summarize reddit post contents",
  "permissions": [
	"tabs"
  ],
  "browser_action": {
      "default_title": "Reddit Summarize",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
